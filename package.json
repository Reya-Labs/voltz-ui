{
  "name": "voltz-ui",
  "version": "1.0.0",
  "description": "A UI client for the Voltz Protocol",
  "license": "MIT",
  "scripts": {
    "build": "node scripts/build.js",
    "build-storybook": "build-storybook",
    "generate": "graphql-codegen --config graphql-codegen.yaml",
    "ts:check": "tsc --noEmit",
    "lint:check": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier:check": "prettier --check './src/**/*.{ts,tsx}' './scripts/**/*.js' './.storybook/**/*.js'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx}' './scripts/**/*.js' './.storybook/**/*.js'",
    "prepare": "husky install",
    "release": "semantic-release",
    "start": "node scripts/start.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "chromatic": "npx chromatic --project-token=e40b8c3e3eea"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore: Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "resolutions": {
    "@apollo/client": "3.5.1"
  },
  "dependencies": {
    "@apollo/client": "3.5.1",
    "@emotion/react": "^11.7.1",
