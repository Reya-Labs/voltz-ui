{
  "name": "voltz-ui",
  "version": "1.221.2",
  "description": "A UI client for the Voltz Protocol",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn code-quality:check",
    "build": "GENERATE_SOURCEMAP=false node scripts/build.js",
    "build-storybook": "build-storybook",
    "ts:check": "yarn ts:check-types && yarn ts:circular-check",
    "ts:check-types": "tsc --noEmit",
    "ts:check-unused-exports": "npx ts-unused-exports ./tsconfig.json --ignoreFiles='(.*stories|.*mocks|.*mock)' --silent --ignoreTestFiles --allowUnusedTypes",
    "ts:circular-check": "dpdm --no-output --no-tree --no-warning --exit-code circular:1 -T ./src/index.tsx",
    "lint:check": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier:check": "prettier --loglevel silent --check './src/**/*.{ts,tsx}' './scripts/**/*.js' './.storybook/**/*.js'",
    "prettier:fix": "prettier --loglevel silent --write './src/**/*.{ts,tsx}' './scripts/**/*.js' './.storybook/**/*.js'",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "start": "GENERATE_SOURCEMAP=false node scripts/start.js",
    "storybook": "start-storybook -p 6006",
    "test": "TZ=UTC jest --silent",
    "test:coverage": "yarn test --coverage",
    "test:watch": "TZ=UTC jest --watch",
    "generate:coverage-badges": "npx istanbul-badges-readme --silent",
    "code-quality:check": "npx concurrently 'yarn ts:check' 'yarn prettier:check' 'yarn lint:check --quiet'",
    "git:create-release-branch": "git checkout main && git pull && git checkout develop && git pull && git checkout -b release/$FEATURE && git checkout release/$FEATURE && git merge main --no-edit"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "branches": [
      "develop"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "README.md"
          ],
          "message": "chore: Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "resolutions": {
    "@sentry/react": "7.25.0",
    "@sentry/tracing": "7.25.0"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@metamask/detect-provider": "1.2.0",
    "@metamask/jazzicon": "2.0.0",
    "@reduxjs/toolkit": "1.9.0",
    "@sentry/react": "7.25.0",
    "@sentry/tracing": "7.25.0",
    "@voltz-protocol/community-sdk": "1.4.0",
    "@voltz-protocol/sdk-v1-stateless": "1.26.11",
    "@voltz-protocol/sdk-v2": "1.53.3",
    "@voltz-protocol/v1-sdk": "1.170.0",
    "@walletconnect/ethereum-provider": "1.8.0",
    "@walletconnect/web3-provider": "1.8.0",
    "aws-amplify": "4.3.44",
    "brokoli-ui": "1.82.5",
    "copy-to-clipboard": "3.3.3",
    "ethers": "5.5.4",
    "isomorphic-fetch": "3.0.0",
    "lodash.debounce": "4.0.8",
    "lodash.orderby": "4.6.0",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-animate-height": "3.1.0",
    "react-countup": "6.3.2",
    "react-dom": "17.0.2",
    "react-flip-move": "3.0.5",
    "react-gtm-module": "2.0.11",
    "react-redux": "8.0.5",
    "react-responsive": "9.0.2",
    "react-router-dom": "6.9.0",
    "redux": "4.2.0",
    "redux-logger": "3.0.6",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.0",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@sentry/webpack-plugin": "1.20.0",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.12",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "13.5.0",
    "@types/big.js": "6.1.3",
    "@types/isomorphic-fetch": "0.0.36",
    "@types/jest": "27.4.0",
    "@types/lodash.debounce": "4.0.7",
    "@types/lodash.orderby": "4.6.7",
    "@types/luxon": "2.0.9",
    "@types/node": "17.0.9",
    "@types/react": "17.0.53",
    "@types/react-dom": "17.0.19",
    "@types/react-gtm-module": "2.0.1",
    "@types/redux-logger": "3.0.9",
    "@types/uuid": "8.3.4",
    "babel-jest": "27.4.6",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "browserslist": "4.21.3",
    "camelcase": "6.3.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "concurrently": "7.6.0",
    "copy-webpack-plugin": "10.2.1",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.3.1",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "dpdm": "3.10.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "is-ci": "3.0.1",
    "istanbul-badges-readme": "1.8.4",
    "jest": "27.4.7",
    "luxon": "2.3.0",
    "mini-css-extract-plugin": "2.5.0",
    "prettier": "2.4.1",
    "react-app-polyfill": "3.0.0",
    "react-dev-utils": "12.0.1",
    "react-refresh": "0.11.0",
    "resolve": "1.21.0",
    "resolve-url-loader": "4.0.0",
    "semantic-release": "19.0.5",
    "semantic-release-cli": "5.4.4",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.0",
    "ts-jest": "27.1.3",
    "ts-unused-exports": "9.0.4",
    "typescript": "4.5.4",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-dev-server": "4.7.3",
    "webpack-manifest-plugin": "4.1.1",
    "workbox-webpack-plugin": "6.4.2"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">= 16.17.0",
    "yarn": ">= 1.22.0",
    "npm": "please-use-yarn"
  }
}
