{
  "name": "voltz-ui",
  "version": "1.13.1",
  "description": "A UI client for the Voltz Protocol",
  "license": "MIT",
  "scripts": {
    "build": "node scripts/build.js",
    "build-storybook": "build-storybook",
    "generate": "graphql-codegen --config graphql-codegen.yaml",
    "ts:check": "tsc --noEmit",
    "lint:check": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier:check": "prettier --check './src/**/*.{ts,tsx}' './scripts/**/*.js' './.storybook/**/*.js'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx}' './scripts/**/*.js' './.storybook/**/*.js'",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "start": "node scripts/start.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "chromatic": "npx chromatic --project-token=e40b8c3e3eea",
    "sdk:add-local": "npx yalc add @voltz-protocol/v1-sdk",
    "sdk:update-local": "npx yalc update @voltz-protocol/v1-sdk",
    "code-quality:check": "npx concurrently 'yarn ts:check' 'yarn prettier:check' 'yarn lint:check --quiet'"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "branches": [
      "develop"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore: Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "resolutions": {
    "@apollo/client": "3.5.1"
  },
  "dependencies": {
    "@apollo/client": "3.5.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@metamask/detect-provider": "1.2.0",
    "@metamask/jazzicon": "2.0.0",
    "@mui/icons-material": "5.6.0",
    "@mui/material": "5.3.1",
    "@mui/system": "5.3.0",
    "@voltz-protocol/v1-sdk": "1.0.3",
    "@walletconnect/ethereum-provider": "1.8.0",
    "@walletconnect/web3-provider": "1.8.0",
    "aws-amplify": "4.3.13",
    "axios": "0.27.2",
    "chroma-js": "2.1.2",
    "copy-to-clipboard": "3.3.3",
    "ethereumjs-abi": "0.6.8",
    "ethers": "5.5.4",
    "graphql": "16.3.0",
    "graphql-request": "5.0.0",
    "jsbi": "4.2.0",
    "lodash": "4.17.21",
    "luxon": "2.3.0",
    "process": "0.11.10",
    "react": "17.0.2",
    "react-countup": "6.3.2",
    "react-currency-input-field": "3.6.4",
    "react-dom": "17.0.2",
    "react-gtm-module": "2.0.11",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
